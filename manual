5.ROS_NAMESPACE=tb3_1 roslaunch turtlebot3_manipulation_bringup turtlebot3_manipulation_bringup.launch
6.ROS_NAMESPACE=tb3_1 roslaunch turtlebot3_manipulation_moveit_config move_group.launch 
7.ROS_NAMESPACE=tb3_1 roslaunch turtlebot3_manipulation_gui turtlebot3_manipulation_gui.launch

8.roslaunch turtlebot3_nps move_base_two.launch
9.roslaunch turtlebot3_nps navigation_two.launch map_file:=$HOME/workspace/3x3.yaml

